# Decrease volume (Command + Shift + N)
alt + shift - n : osascript -e 'set volume output volume ((output volume of (get volume settings)) - 5)'

# Increase volume (Command + Shift + M)
alt + shift - m : osascript -e 'set volume output volume ((output volume of (get volume settings)) + 5)'

# brave custom
cmd + shift - o : open -na "Brave Browser"

# open discort
cmd + shift - i : open -a "Discord"
